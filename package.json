{
  "name": "langgraph-python-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:ui\" \"pnpm run dev:agent\" \"pnpm run dev:api\" --names ui,agent,api --prefix-colors blue,green,yellow --kill-others",
    "dev:debug": "LOG_LEVEL=debug pnpm run dev",
    "dev:agent": "cd backend && uv run python -m langgraph_cli dev --port 8123 --no-browser",
    "dev:api": "cd backend && uv run python -m uvicorn api.main:app --reload --port 8000",
    "dev:ui": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "install:agent": "sh ./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "pnpm run install:agent"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.50.1",
    "@copilotkit/react-ui": "1.50.1",
    "@copilotkit/runtime": "1.50.1",
    "@mantine/core": "8.3.11",
    "@mantine/hooks": "8.3.11",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@langchain/langgraph-cli": "1.0.4",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.27.0",
  "pnpm": {
    "overrides": {
      "@langchain/langgraph-cli": "1.0.4"
    }
  }
}
